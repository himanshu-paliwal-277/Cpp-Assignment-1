// Q1 - Take 2 integer values in two variables x and y and print their product.

#include <iostream>
using namespace std;

int main()
{
    int x, y;
    cout << "Enter a value of x: " << endl;
    cin >> x;
    cout << "Enter a value of y: " << endl;
    cin >> y;
    cout << "Product = " << x * y << endl;
    return 0;
}